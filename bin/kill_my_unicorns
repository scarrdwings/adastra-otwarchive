#!/bin/sh
me="vagrant"
unicorn_master_pid=`ps ax | grep $me.sock | grep master | awk '{print $1}'`
kill $unicorn_master_pid
#now the collection watcher
collection_watcher_pid=`ps ax|grep collection_watcher | grep -v grep | awk '{print $1}'`
kill $collection_watcher_pid
#now the pseud_watcher
pseud_watcher_pid=`ps ax|grep pseud_watcher | grep -v grep | awk '{print $1}'`
kill $pseud_watcher_pid
